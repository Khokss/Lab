  <X-PRE-PROCESS cmd="set" data="internal_ssl_enable={{ internal_ssl_enable }}"/>

  <!-- External SIP Profile -->
  <X-PRE-PROCESS cmd="set" data="external_auth_calls={{ external_auth_calls }}"/>
